{% if attributes.id == "webform-submission-contact-add-form" %}
<section id="contact" class="contact">
  <div class="container" data-aos="fade-up">
    <div class="section-title">
      <h2>Contact Us</h2>
      <p>
        Managing your fleet at tip of your fingers.<br>
        We are on a quest for making marketing tools smarter, more powerful and more elegant.
      </p>
    </div>

    <div class="row">
      <div class="col-lg-5 d-flex align-items-stretch">
        <div class="info">
          <div class="address">
            <i class="icofont-google-map"></i>
            <h4>Location:</h4>
              <p>PO Box 1075,Boroko NCD,<br />Sec 451, Lot 7,Cameron Road,<br />Gordons, NCD 121,<br />Port Moresby PNG<br /><br />
          </div>

          <div class="email">
            <i class="icofont-envelope"></i>
            <h4>Email:</h4>

            <p>info@digitec.com.pg</p>
          </div>

          <i class="icofont-phone"></i>
          <div class="phone">
            <h4>Call:</h4>

            <p>+675 3139603</p>
          </div>
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d12097.433213460943!2d-74.0062269!3d40.7101282!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0xb89d1fe6bc499443!2sDowntown+Conference+Center!5e0!3m2!1smk!2sbg!4v1539943755621"
            frameborder="0"
            style="border: 0; width: 100%; height: 40%; margin-bottom:20px"
            allowfullscreen
          ></iframe>
        </div>
      </div>
 
      <div class="col-lg-7 mt-5 mt-lg-0 d-flex align-items-stretch php-email-form">
        
        <form{{ attributes }} data-drupal-form-fields="edit-name,edit-email,edit-subject,edit-message,edit-actions-submit" data-destroy="false">
          {{ children }}
        </form>
        
      </div>
    </div>
  </div>
</section>
<!-- End Contact Section -->
{% else %}
<form{{ attributes }} data-drupal-form-fields="edit-name,edit-email,edit-subject,edit-message,edit-actions-submit" data-destroy="false">
    {{ children }}
</form>
{% endif %}