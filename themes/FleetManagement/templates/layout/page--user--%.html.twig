{{ attach_library("fleet_management_theme/back-style") }}
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/modernizr.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/jquery-3.2.1.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/popper.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/bootstrap.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/jquery-jvectormap.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/jquery-jvectormap-world-mill.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/custom-vector.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/charts/js/raphael-min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/charts/js/morris.min.js"></script>
<link rel="stylesheet" href="/AWFleetManagement/themes/FleetManagement/assets/back/charts/css/morris.css">
<script src="{{ url('<front>') }}themes/FleetManagement/assets/back/css/sweetalert2.min.css"></script>
<script src="{{ url('<front>') }}themes/FleetManagement/assets/back/js/sweetalert2.all.js"></script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<div class="wrapper">
    <!-- Sidebar -->
    <nav id="sidebar" class="lorvens-bg">
      <div class="sidebar-header">
        {% if role == 'company' %} {% set home_url = base_url ~
        '/company/dashboard' %} {% elseif role == 'operator' %} {% set home_url =
        base_url ~ '/operator/dashboard' %} {% elseif role == 'administrator' %}
        {% set home_url = base_url ~ '/admin/dashboard' %} {% endif %}
        <a href="{{ home_url }}"><h5 class="logo" alt="logo">DIGIFLEET</h5></a>
        <a href="{{ home_url }}"><h5 class="minimal-logo text-align" alt="logo"><strong>DF</strong></h5></a>
      </div>
      {% if expiry_status != 'expired' %}
      <ul class="list-unstyled components">
        {{
          page.header
        }}
      </ul>
      {% endif %}
    </nav>
    <!-- /Sidebar -->
    <!-- Page Content -->
    <div id="content">
      <!-- Top Navigation -->
      <nav class="navbar navbar-default">
        <div class="container-fluid">
          <div class="navbar-header">
            <span class="ti-menu" id="sidebarCollapse"></span>
          </div>
          <ul class="nav justify-content-end">
            <li class="nav-item">
              <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
                {% if notifications | json_decode %}
                <span class="ti-announcement"><span class="notification-badges"></span></span>
                {% else %}
                <span class="ti-announcement"></span>
                {% endif %}
              </a>
              <div class="dropdown-menu lorvens-box-shadow notifications animated flipInY">
                <h5>Notifications</h5>
                <div class="notification-menu">
                {% if notifications | json_decode %}
                {% for i in 0..4 %}
                  {% set notification = notifications | json_decode %}
                  {% if notification[i][0].type %}
                  <p class="dropdown-header font-weight-bold text-left bg-lightgrey">{{ notification[i][0].type }}</p>
                  {% endif %}
                  {% for j in 0..2 %}
                    {% if notification[i][j] %}
                      <a class="dropdown-item" href="#">
                        <span class="{{ notification[i][j].icon }}"></span> {{ notification[i][j].title }}</a>
                    {% endif %}
                  {% endfor %}
                {% endfor %}
                  <hr class="m-0">
                  <a href="{{ base_url }}/notifications" class="dropdown-header text-center text-primary font-weight-bold text-left m-auto">View More</a>
                {% else %}
                  <a class="dropdown-item" href="#">
                    <span class="ti-info"></span> No Notification Yet!</a>
                {% endif %}
                  </div>
              </div>
            </li>
            <li class="nav-item">
              <a
                class="nav-link dropdown-toggle"
                data-toggle="dropdown"
                href="#"
                role="button"
                aria-haspopup="true"
                aria-expanded="false"
              >
                <span class="ti-user"></span>
              </a>
              <div
                class="dropdown-menu lorvens-box-shadow profile animated flipInY"
              >
                <h5>{{ username }}</h5>
                <!-- <a class="dropdown-item" href="#">
                  <span class="ti-settings"></span> Settings</a
                > -->
                <a class="dropdown-item" href="{{ base_url }}/help">
                  <span class="ti-help-alt"></span> Help</a
                >
                <a class="dropdown-item" href="{{ base_url }}/user/logout">
                  <span class="ti-power-off"></span> Logout</a
                >
              </div>
            </li>
          </ul>
        </div>
      </nav>
      <!-- /Top Navigation -->
      <!-- Breadcrumb -->
      <ol class="breadcrumb">
        <li class="breadcrumb-item">
          <a href="index.html">
            <span class="ti-home"></span>
          </a>
        </li>
        <li class="breadcrumb-item active">Dashboard</li>
      </ol>
      <!-- /Breadcrumb -->
      <!-- Main Content -->
      <input
      id="package"
      name="package"
      type="hidden"
      value="{{ expiry_status }}"
    />
    {% if expiry_status != 'expired' %}
      {{ page.content }}
    {% endif %}
      <!-- /Main Content -->
    </div>
    <!-- /Page Content -->
  </div>
  <!-- Back to Top -->
  
  <!-- 
  File Include
  -->
  <a id="back-to-top" href="#" class="back-to-top fenil">
    <span class="ti-angle-up"></span>
  </a>
<script>
  var package = document.getElementById("package").value;
  // console.log(package);
  if (package == "expired") {
    Swal.fire({
      title: 'Subscribe Package!',
      text: "Please Choose Your Package!",
      icon: 'info',
      showCancelButton: true,
      confirmButtonColor: '#3085d6',
      cancelButtonColor: '#d33',
      confirmButtonText: 'Subscribe!',
      cancelButtonText: 'Logout',
      allowOutsideClick: false,
      allowEscapeKey: false,
      allowEnterKey:false,
      showClass: {
        popup: 'swal2-noanimation',
        backdrop: 'swal2-noanimation'
      },
      hideClass: {
        popup: '',
        backdrop: ''
      }
    }).then((result) => {
      console.log(result);
      if (result.isConfirmed) {
        // window.location.href = "https://www.google.com/";
        window.location.href = '{{ base_url }}/renew';
      }
      if(result.isDismissed){
        // window.location.href = "https://www.google.com/";
        window.location.href = '{{ base_url }}/user/logout';
      }
    })
  }
</script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/custom.js"></script>