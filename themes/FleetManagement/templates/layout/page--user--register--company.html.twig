{% if not logged_in %} {% if page.header %}
{{ attach_library("fleet_management_theme/front-style") }}
<header id="header" class="fixed-top" style="background: rgba(40, 58, 90)">
  <div class="container-fluid px-5 d-flex align-items-center">
    <h1 class="logo mr-auto"><a href="{{ url('<front>') }}">DIGIFLEET</a></h1>
    {{ page.header }}
  </div>
</header>
{% endif %} {% if page.content %}
<div class="container" style="margin-top: 8%">
  {{ page.content }}
</div>
{% endif %}

<footer id="footer">
  {% if page.features_third and is_front %}
  <div class="footer-newsletter">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-6">
          <h4>Join Our Newsletter</h4>
          <p>
            Tamen quem nulla quae legam multos aute sint culpa legam noster
            magna
          </p>
          {{ page.features_third }}
        </div>
      </div>
    </div>
  </div>
  {% endif %}

  <div class="footer-top">
    <div class="container">
      <div class="row">
        {% if page.bottom_first %}
        <div class="col-lg-4 col-md-6 footer-contact">
          {{ page.bottom_first }}
        </div>
        {% endif %} {% if page.bottom_second %}
        <div class="col-lg-4 col-md-6 footer-links">
          <h4>Links</h4>
          {{ page.bottom_second }}
        </div>
        {% endif %}

        <div class="col-lg-4 col-md-6 footer-links">
          <h4>Our Social Networks</h4>
          <p>
            Cras fermentum odio eu feugiat lide par naso tierra videa magna
            derita valies
          </p>
          <div class="social-links mt-3">
            <a href="{{ facebook_url }}" class="facebook"
              ><i class="bx bxl-facebook"></i
            ></a>
            <a href="{{ instagram_url }}" class="instagram"
              ><i class="bx bxl-instagram"></i
            ></a>
            <a href="{{ linkedin_url }}" class="linkedin"
              ><i class="bx bxl-linkedin"></i
            ></a>
            <a href="{{ twitter_url }}" class="twitter"
              ><i class="bx bxl-twitter"></i
            ></a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="container footer-bottom clearfix">
    {% if page.footer_first %}
    {{ page.footer_first }}
    {% endif %} {% if page.footer_second %}
    {{ page.footer_second }}
    {% endif %}
  </div>
</footer>
{% elseif logged_in %}
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/modernizr.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/jquery-3.2.1.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/popper.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/bootstrap.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/jquery-jvectormap.min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/jquery-jvectormap-world-mill.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/js/custom-vector.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/charts/js/raphael-min.js"></script>
<script src="/AWFleetManagement/themes/FleetManagement/assets/back/charts/js/morris.min.js"></script>
<link rel="stylesheet" href="/AWFleetManagement/themes/FleetManagement/assets/back/charts/css/morris.css">
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<div class="wrapper">
  <!-- Sidebar -->
  <nav id="sidebar" class="lorvens-bg">
    <div class="sidebar-header">
      {% if role == 'company' %} {% set home_url = base_url ~
      '/company/dashboard' %} {% elseif role == 'operator' %} {% set home_url =
      base_url ~ '/operator/dashboard' %} {% elseif role == 'administrator' %}
      {% set home_url = base_url ~ '/admin/dashboard' %} {% endif %}
      <a href="{{ home_url }}"><h5 class="logo" alt="logo">DIGIFLEET</h5></a>
      <a href="{{ home_url }}"><h5 class="minimal-logo" alt="logo"><strong>DF</strong></h5></a>
    </div>
    {% if expiry_status != 'expired' %}
    <ul class="list-unstyled components">
      {{
        page.header
      }}
    </ul>
    {% endif %}
  </nav>
  <!-- /Sidebar -->
  <!-- Page Content -->
  <div id="content">
    <!-- Top Navigation -->
    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <div class="navbar-header">
          <span class="ti-menu" id="sidebarCollapse"></span>
        </div>
        <ul class="nav justify-content-end">
          <li class="nav-item">
            <a
              class="nav-link dropdown-toggle"
              data-toggle="dropdown"
              href="#"
              role="button"
              aria-haspopup="true"
              aria-expanded="false"
            >
              <span class="ti-user"></span>
            </a>
            <div
              class="dropdown-menu lorvens-box-shadow profile animated flipInY"
            >
              <h5>{{ username }}</h5>
              <a class="dropdown-item" href="#">
                <span class="ti-settings"></span> Settings</a
              >
              <a class="dropdown-item" href="#">
                <span class="ti-help-alt"></span> Help</a
              >
              <a class="dropdown-item" href="{{ base_url }}/user/logout">
                <span class="ti-power-off"></span> Logout</a
              >
            </div>
          </li>
        </ul>
      </div>
    </nav>
    <!-- /Top Navigation -->
    <!-- Breadcrumb -->
    <ol class="breadcrumb">
      <li class="breadcrumb-item">
        <a href="index.html">
          <span class="ti-home"></span>
        </a>
      </li>
      <li class="breadcrumb-item active">Dashboard</li>
    </ol>
    <!-- /Breadcrumb -->
    <!-- Main Content -->
    {{ page.content }}
    <!-- /Main Content -->
  </div>
  <!-- /Page Content -->
</div>
<!-- Back to Top -->

