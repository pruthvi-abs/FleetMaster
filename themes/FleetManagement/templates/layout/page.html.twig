{% if not logged_in %}
{{ attach_library("fleet_management_theme/front-style") }}
{% if page.header %}
<header id="header" class="fixed-top" style="background: #37517e">
  <div class="container-fluid px-5 d-flex align-items-center">
    <h1 class="logo mr-auto"><a href="{{ url('<front>') }}">DIGIFLEET</a></h1>
    {{ page.header }}
    <div style="margin-left: 50px">
      <a href="{{ url('<front>') }}user/login" class="btn btn-success">login</a>
      <a href="{{ url('<front>') }}register/company" class="btn btn-primary"
        >Register</a
      >
    </div>
    <!-- <a href="/user/logout" class="btn btn-outline-danger">Logout</a> -->
  </div>
</header>
{% endif %}
<!-- End Header -->
{% if is_front and show_slideshow %}
<section id="hero" class="d-flex align-items-center">
  <div class="container">
    <div class="row">
      {% for slider_contents in slider_content %}
      {{ slider_contents | raw }}
      {% endfor %}
    </div>
  </div>
</section>
{% endif %}
<!-- End Hero -->

<main id="main">
  <!-- ======= content Section ======= -->
  {% if page.content and not is_front %}
  <section id="content" class="section-bg">
    <div class="container" data-aos="fade-up">
      {{ page.content }}
    </div>
  </section>
  {% endif %}
  <!-- ======= content Section ======= -->

  <!-- ======= Services Section ======= -->

  {% if page.features_first and is_front %}
  <section id="services" class="services section-bg">
    <div class="container" data-aos="fade-up">
      {{ page.features_first }}
    </div>
  </section>
  {% endif %}
  <!-- End Services Section -->
  <!-- ======= Cta Section ======= -->
  {% if page.features_second and is_front %}
  <section id="cta" class="cta">
    <div class="container" data-aos="zoom-in">
      <div class="row">
        {{ page.features_second }}
      </div>
    </div>
  </section>
  {% endif %}
  <!-- End Cta Section -->
</main>
<!-- End #main -->

<!-- ======= Footer ======= -->
<footer id="footer">
  {% if page.features_third and is_front %}
  <div class="footer-newsletter">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-6">
          <h4>Join Our Newsletter</h4>
          <p>
            Tamen quem nulla quae legam multos aute sint culpa legam noster
            magna
          </p>
          {{ page.features_third }}
        </div>
      </div>
    </div>
  </div>
  {% endif %}

  <div class="footer-top">
    <div class="container">
      <div class="row">
        {% if page.bottom_first %}
        <div class="col-lg-4 col-md-6 footer-contact">
          {{ page.bottom_first }}
        </div>
        {% endif %} {% if page.bottom_second %}
        <div class="col-lg-4 col-md-6 footer-links">
          <h4>Links</h4>
          {{ page.bottom_second }}
        </div>
        {% endif %}

        <div class="col-lg-4 col-md-6 footer-links">
          <h4>Our Social Networks</h4>
          <p>
            Cras fermentum odio eu feugiat lide par naso tierra videa magna
            derita valies
          </p>
          <div class="social-links mt-3">
            <a href="{{ facebook_url }}" class="facebook"
              ><i class="bx bxl-facebook"></i
            ></a>
            <a href="{{ instagram_url }}" class="instagram"
              ><i class="bx bxl-instagram"></i
            ></a>
            <a href="{{ linkedin_url }}" class="linkedin"
              ><i class="bx bxl-linkedin"></i
            ></a>
            <a href="{{ twitter_url }}" class="twitter"
              ><i class="bx bxl-twitter"></i
            ></a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="container footer-bottom clearfix">
    {% if page.footer_first %}
    {{ page.footer_first }}
    {% endif %} {% if page.footer_second %}
    {{ page.footer_second }}
    {% endif %}
  </div>
</footer>

<!-- End Footer -->

<a href="#" class="back-to-top"><i class="ri-arrow-up-line"></i></a>
<div id="preloader"></div>

{% else %}

{{ attach_library("fleet_management_theme/back-style") }}
