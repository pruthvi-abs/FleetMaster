
<section id="about-us-bottom">
  <div class="container">
    <div class="row">
        <div class="left_side col-md-4 col-sm-6 col-xs-12 wow fadeInUp" data-wow-delay="0.3s">
          <img src="{{ file_url(content.field_image['#items'].entity.uri.value) }}" class="img-responsive">
        </div>
        <div class="right_side col-md-8 col-sm-6 col-xs-12 wow fadeInUp" data-wow-delay="0.6s">
          <h2>{{ content.field_description.0|render|striptags }}</h2>
          <ul>
            <li>{{ content.field_title.0 }} Countries</li>
            <li>{{ content.field_telephone_no.0 }} Branches</li>
            <li>{{ content.field_company_name.0 }} Logistis Serrvices</li>
            <li><a href="{{ content.field_url[0]['#url']|render }}" class="enquiry-btn custom-btn pull-right wow fadeInLeft" data-wow-delay="0.9s">{{ content.field_url[0]['#title'] }} <i class="fa fa-angle-right"></i></a></li>
          </ul>
        </div>
    </div>
  </div>
</section>
