<!-- Certificate -->
<section id="certificate">
  <div class="container">
  <div class="row">
    <div class="certificate_details">
      <div class="left_align col-md-6 col-sm-12 col-xs-12 wow fadeInLeft" data-wow-delay="0.3s">
        <div class="certificate_left_side">
          <img src="{{ file_url(content.field_image['#items'].entity.uri.value) }}" class="img-responsive">
        </div>
      </div>
      <div class="right_align col-md-6 col-sm-12 col-xs-12 wow fadeInUp" data-wow-delay="0.6s">
        <div class="certificate_right_side">
          <div class="certificate_right_side_image">
            <img src="{{ file_url(content.field_certificate_image['#items'].entity.uri.value) }}" class="img-responsive">  
          </div>
           <div class="certificate_right_side_content text-center">
             <p><strong>{{ content.field_title.0 }}</strong></p>
             <p>{{ content.field_telephone_no.0 }}</p>
           </div>
        </div>
      </div>
    </div>
  </div>
  </div>
</section>
