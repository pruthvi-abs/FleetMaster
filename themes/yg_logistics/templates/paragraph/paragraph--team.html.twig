<div class="col-md-3 col-sm-6 col-xs-12 wow fadeInUp" data-wow-delay="0.2s">               
  <div class="hovereffect">
    <img src="{{ file_url(content.field_image['#items'].entity.uri.value) }}" class="img-responsive">
  </div>
  <div class="team_list_caption">
    <h3><a href="">{{ content.field_title.0 }}</a></h3>
    <div class="services-caption">
      <p>{{ content.field_service_title_slogan.0 }}</p>
      <div class="line"></div>
        {% if content.field_url.0|render is not empty %}
          <a href="{{ content.field_url[0]['#url']|render }}"><i class="fa fa-facebook"></i></a>
        {% endif %}
        {% if content.field_twitter.0|render is not empty %}
          <a href="{{ content.field_twitter[0]['#url']|render }}"><i class="fa fa-twitter"></i></a>
        {% endif %}
        {% if content.field_google_plus.0|render is not empty %}
          <a href="{{ content.field_google_plus[0]['#url']|render }}"><i class="fa fa-google-plus"></i></a>
        {% endif %}
     </div>
   </div>
</div>